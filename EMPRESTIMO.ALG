algoritmo "emprestimo"
var  emprestimo, parcela: real

inicio
      Escreva(" Qual o valor do empréstimo ?")
      Leia (emprestimo)
      Escreva("Em quantas parcelas ? ")
      Leia(parcela)
      emprestimo <- (emprestimo * 20/100 + emprestimo) / parcela)
      Escreva(" O valor das parcelas é de R$ ", emprestimo:5:2)
fimalgoritmo